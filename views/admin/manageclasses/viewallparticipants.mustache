<html>
<head>
  {{> partials/header }}
</head>

<body class="container-fluid py-5 bg-light">
  <div>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Email</th>
          <th scope="col">Phone Number</th>
        </tr>
      </thead>
      <tbody>
        {{#participant}}
          <tr>
            <td>{{_id}}</td>
            <td>{{firstname}}</td>
            <td>{{lastname}}</td>
            <td>{{email}}</td>
            <td>{{phonenumber}}</td>
          </tr>
        {{/participant}}
      </tbody>
    </table>
  </div>
</body>
</html>
